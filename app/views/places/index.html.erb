<h1>Places</h1>

<ul>
  <% for place in @places %>
    <li><a href="/places/<%= place["id"] %>"><%= place["name"] %></a></li>
  <% end %>
</ul>
 <% if @current_user %>
      <a href="/places/new" class="btn btn-outline-primary btn-lg ">New Place</a>
          <% else %>
  <% end %>

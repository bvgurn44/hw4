<h1>New Place</h1>
<% if @current_user %>
  <%= form_with :model => @place do |form| %>
  <p>
    <%= form.label "name" %>
    <%= form.text_field "name" %>
  </p>
  <p>
        <%= form.submit "Create Place", :class => "btn btn-primary" %>
  </p>
  <% end %>
<% else %>
  <p>You must be logged in to add a new place.</p>
<%end%>
<p><a href="/places">Back to Places</a></p>